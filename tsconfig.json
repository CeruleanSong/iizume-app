{
	"compilerOptions": {
	    "allowJs": true,
	    "allowSyntheticDefaultImports": true,
	    "isolatedModules": true,
		
	    "esModuleInterop": true,
		"resolveJsonModule": true,
	    "jsx": "react",
	    "lib": ["es6"],
		
		"noEmit": true,
	    "strict": true,
	    "target": "esnext",
		"module": "esnext",
		"moduleResolution": "node"
	},
	"exclude": [
		"node_modules",
		"babel.config.js",
		"metro.config.js",
		"jest.config.js"
	]
}
